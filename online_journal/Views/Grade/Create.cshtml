@model online_journal.Models.Grade

@{
    ViewData["Title"] = "Doxil kardani baho";
}

<h2>@ViewData["Title"]</h2>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="StudentId">Xonanda</label>
        <select asp-for="StudentId" class="form-control" asp-items="ViewBag.Students"></select>
    </div>
    <div class="form-group">
        <label asp-for="SubjectId">Fan</label>
        <select asp-for="SubjectId" class="form-control" asp-items="ViewBag.Subjects"></select>
    </div>
    <div class="form-group">
        <label asp-for="Score">Baho</label>
        <input asp-for="Score" class="form-control" />
    </div>
    <button type="submit" class="btn btn-success">Sabt kardan</button>
    <a asp-action="Index" class="btn btn-secondary">Ba qafo</a>
</form>
