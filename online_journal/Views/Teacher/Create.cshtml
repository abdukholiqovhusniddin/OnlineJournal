@model online_journal.Models.Teacher

@{
    ViewData["Title"] = "Doxil kardani xonanda";
}

<h1>@ViewData["Title"]</h1>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="FullName"></label>
        <input asp-for="FullName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PhoneNumber"></label>
        <input asp-for="PhoneNumber" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="SubjectId">Fan</label>
        <select asp-for="SubjectId" asp-items="ViewBag.Subjects" class="form-control">
            <option value="">Intixob</option>
        </select>
    </div>
    <button type="submit" class="btn btn-success">Sabt kardan</button>
</form>
